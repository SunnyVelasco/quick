<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="apps.ejemplo.marip.quick">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_BACKGROUND_LOCATION" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="apps.ejemplo.marip.quick.AgregarDesdeAgenda"></activity>
        <activity android:name="apps.ejemplo.marip.quick.PasarContactos" />
        <activity android:name="apps.ejemplo.marip.quick.opciones_contacto" />
        <activity
            android:name="apps.ejemplo.marip.quick.bienvenida"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="apps.ejemplo.marip.quick.MainActivity" />
        <activity android:name="apps.ejemplo.marip.quick.registro" />
        <activity android:name="apps.ejemplo.marip.quick.boton_emergencia" />
        <activity android:name="apps.ejemplo.marip.quick.menu" />
        <activity
            android:name="apps.ejemplo.marip.quick.ejem"
            android:label="@string/title_activity_ejem"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="apps.ejemplo.marip.quick.contactos" />
        <activity
            android:name="apps.ejemplo.marip.quick.Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="apps.ejemplo.marip.quick.AgregadosContactos"
            android:label="@string/title_activity_agregados_contactos"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>