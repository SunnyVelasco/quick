<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fondo2"
    tools:context=".bienvenida"
    >


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="336dp"
        android:layout_height="290dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="176dp"
        android:background="@drawable/personas" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="106dp"
        android:layout_marginBottom="472dp"
        android:text="Una herramienta para ti y para tu seguridad"
        android:textColor="@color/ColorText"
        android:textStyle="italic" />

    <Button
        android:id="@+id/Iniciar_sesión"
        android:layout_width="239dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="91dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="102dp"
        android:background="@drawable/boton_estilo"
        android:text="INICIAR SESIÓN "
        android:textSize="20dp" />

    <Button
        android:id="@+id/Registrarse"
        android:layout_width="239dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="91dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/boton_estilo"
        android:text="Registrarse"
        android:textSize="20dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="294dp"
        android:layout_height="304dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="91dp"
        android:layout_marginTop="102dp"
        app:srcCompat="@drawable/quickblanco" />
</RelativeLayout>